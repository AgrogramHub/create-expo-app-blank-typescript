# commit format

COMMIT_TEMPLATE_FILE="$(dirname "$0")/commit-template.txt"
if [ -f "$COMMIT_TEMPLATE_FILE" ]; then
COMMIT_TEMPLATE=$(cat "$COMMIT_TEMPLATE_FILE")
else
echo "Commit template file not found."
exit 1
fi

